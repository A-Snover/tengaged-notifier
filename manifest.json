{
    "name": "Tengaged Notifier",
    "version": "1.0",
    "description": "Notifies when in-game system notifications pop up.",
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": [
                //"*://tengaged.com/game/*"
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "manifest_version": 2,
    "web_accessible_resources": ["resource://ding.wav"]
}
